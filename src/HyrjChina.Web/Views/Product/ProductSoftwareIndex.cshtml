@model HyrjChina.Web.Models.ProductSoftwareIndexViewMode
@{
    Layout = null;
}
<div class="row">
    @for (int i = 0; i < Model.Products.Count(); i++)

    {
        if (i % 4 >= 2)
        {
            @Html.Partial("ProductSummary", Model.Products[i], new ViewDataDictionary { { "css", "col-lg-3" } })
        }
        else
        {
            @Html.Partial("ProductSummary", Model.Products[i], new ViewDataDictionary { { "css", "" } })
        }
    }

</div>